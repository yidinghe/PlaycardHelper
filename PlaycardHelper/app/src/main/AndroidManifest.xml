<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yidinghe.android.playcardhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cap"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.NewGameActivity"></activity>
        <activity android:name=".ui.activities.GameActivity"></activity>
        <activity android:name=".ui.activities.RankActivity"></activity>
        <activity android:name=".ui.activities.AboutActivity"></activity>
        <activity android:name=".ui.activities.HelpActivity"></activity>
    </application>

</manifest>